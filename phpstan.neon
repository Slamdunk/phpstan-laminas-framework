includes:
    - vendor/phpstan/phpstan-phpunit/extension.neon

parameters:
    level: max
    paths:
        - src/
        - tests/
    excludes_analyse:
        - tests/Rules/Laminas/ServiceManagerGetMethodCallRule/
        - tests/Rules/Laminas/PluginManagerGetMethodCallRule/
        - tests/TestAsset/
        - tests/LaminasIntegration/data/
